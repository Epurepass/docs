/* overrides */
/* TODO sectioning into arrays is probably unecessary */
{
  "cards": [
    {
      "id": "DELETE /api/cards/:card_id",
      "description": "Archives a card.",
      "note": "This endpoint does not delete the card from the database!",
    },
    {
      "id": "PUT /api/cards/:card_id/move",
      "description": "Moves a card to new destination and/or position.",
      "note": "some attrs are required",
    },
    {
      "id": "PUT /api/cards/:card_id/add_to_source",
      "description": "Adds a card to a source repository.",
      "note": "some attrs are required",
    },
    {
      "id": "POST /api/card_relations",
      "description": "Creates a relationship between cards.",
      "note": "requires both card_ids",
    }
  ],
  "notifications": [
    {
      "id": "GET /api/notifications/",
      "note": "Notifications have objectIds!"
    },
    {
      "id": "PUT /api/notifications/:_notificationId",
      "note": "Notifications have objectIds!"
    },
    {
      "id": "DELETE/ api/notifications/:_notificationId",
      "note": "Notifications have objectIds!"
    },
  ],
  "projects": [
    {
      "id": "GET /api/projects/:project_id/collaborators",
      "description": "Returns a list of people who are possible collaborators on a project, including GitHub team members who are not on Zube",
    },
    {
      "id": "GET /api/projects/:project_id/power",
      "description": "Powerful endpoint for querying cards across a project"
    }
  ],
  "sources": [
    {
      "id": "GET /api/sources/:source_id/collaborators",
      "description": "Returns a list of all GitHub collaborators for a source."
    }
  ],
  "users": [
    {
      "id": "POST /api/users/:client_id/tokens",
      "description": "Creates a new API token"
    }
  ],
  "workspaces": [
    {
      "id": "GET /api/workspaces/:workspace_id/categories",
      "description": "Returns a list of categories with their arrays of card_ids.",
      "note": "Categories have objectIds!"
    },
    {
      "id": "GET /api/workspaces/:workspace_id/categories/:_categoryId",
      "description": "Returns a category with its array of card_ids.",
      "note": "Categories have objectIds!"
    },
    {
      "id": "GET /api/workspaces/:workspace_id/categories_metadata",
      "description": "Returns a list of categories without their arrays of card_ids.",
      "note": "Categories have objectIds!"
    },
    {
      "id": "GET /api/workspaces/:workspace_id/categories_metadata/:_categoryId",
      "description": "Returns a category without its array of card_ids.",
      "note": "Categories have objectIds!"
    }
  ],

}
